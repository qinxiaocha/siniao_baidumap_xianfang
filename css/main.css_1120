@charset "utf-8";
/* CSS Document */

.wapperbox{width:1000px;height-:5000px;margin:0 auto; background:#fff;}


/*header css*/
.header{ padding:0 0 0 6px;margin-bottom:5px; position:relative;}
.logo{ float:left;}
.navbox{ float:right;width-:630px; padding-top:50px;}
.navbox ul{}
.navbox ul li{ float:left; background:url(../images/libg.jpg) left center no-repeat;}
.navbox ul li.otherli{background:none;}
.navbox ul li a{color:#333; padding:2px 20px; display:block;}
.navbox ul li a:hover{color:#009da0;}
.navbox ul li a.current{color:#009da0;}

.lan_chen{width:40px; height:30px;position:absolute; top:20px;right:20px;}
.lan_chen a.cha{ display:block;width:17px;height:17px; float:left; margin-right:5px; background:url(../images/ch.jpg) left top no-repeat;}
.lan_chen a.ena{display:block;width:17px;height:17px; float:left; background:url(../images/en.jpg) left top no-repeat;}

.lan_chen a.cha:hover{ background:url(../images/ch_hov.jpg) left top  no-repeat; cursor:pointer;}
.lan_chen a.ena:hover{ background:url(../images/en_hov.jpg) left top  no-repeat; cursor:pointer;}
.lan_chen a.cha.current{ display:block;width:17px;height:17px; float:left;background:url(../images/ch_hov.jpg) left top  no-repeat; cursor:pointer;}
.lan_chen a.ena.current{display:block;width:17px;height:17px; float:left; background: url(../images/en_hov.jpg) left top no-repeat; cursor:pointer;}


/*index content css*/
.main_img{width:100%; margin-bottom:6px;}
.div_imgt{height:245px; margin-bottom:5px;}
.pordiv1{ position:relative; float:left; margin-right:5px;width:495px;height:245px; overflow:hidden;}
.pordiv2{ position:relative; float:left; margin-right:5px;width:246px;height:245px;overflow:hidden;}
.pordiv3{position:relative; float:left; margin-right:0px;width:249px;height:245px;overflow:hidden;}
.pordiv4{position:relative; float:left; margin-right:0px;width:495px;height:245px;overflow:hidden; margin-bottom:5px;}
.pordiv5{position:relative; float:left; margin-right:0px;width:244px;height:250px;overflow:hidden; margin-bottom:5px; margin-right:5px;}
.pordiv6{position:relative; float:left; margin-right:0px;width:245px;height:250px;overflow:hidden; margin-bottom:5px;}
.pordiv7{position:relative; float:left; margin-right:0px;width:499px;height:499px;overflow:hidden; }

.hoveimg{position:absolute; left:0; top:0; display:none;}

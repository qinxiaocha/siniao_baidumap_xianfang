.fix{position:fixed;top:0px}
.div_imgt .pordiv img-{margin-right:5px; float:left;}
.div_imgb{height:500px;}
.img_left{ width:495px; float:left;}
.img_right{width:499px; float:right;}

.con{width:100%;height:303px; padding-bottom:13px;}
.con_left{width:495px; float:left;}
.con_left span.spanbg{width:100%;height:39px; background:url(../images/spanbg.jpg) left top no-repeat; display:block;}
.con_left ul{ margin-top:9px;}
.con_left ul li{ width:100%; background:url(../images/liimgbg.jpg) left bottom repeat-x;color:#000; padding-bottom:5px;}
.con_left ul li span{ display:block;width:100%;height:35px;line-height:35px; text-indent:35px; background:url(../images/conlibf.jpg) 15px center no-repeat;}
.con_left ul li span em{font-size:10px;}
.con_left ul li a:hover{ text-decoration:underline;}

.con_right{width:500px; float:left;}
.con_right span.spandp{ width:100%; height:39px; display:block; background:url(../images/dpimg.jpg) left top no-repeat;}
.dpcontent{width:100%;}
.dpcontent img{ float:left; margin-top:13px;}
.dpcontent a:hover{ display:block;opacity:0.5;filter:progid:DXImageTransform.Microsoft.Alpha(Opacity=50);}
.iconul{ float:left; padding-left:28px; margin-top:-30px;}
.iconul li{ margin-bottom:25px;}

.iconul li.libg01 a, .iconul li.libg02 a, .iconul li.libg03 a, .iconul li.libg04 a{display:block;font-size:18px; color:#333; text-indent:70px;}
.iconul li.libg01 a:hover, .iconul li.libg02 a:hover, .iconul li.libg03 a:hover, .iconul li.libg04 a:hover{color:#009da0;}
.iconul li.libg01{height:50px; line-height:50px; background:url(../images/liimg01.jpg) left center no-repeat; }
.iconul li.libg02{height:55px; line-height:55px; background:url(../images/liimg02.jpg) 3px center no-repeat;} 
.iconul li.libg03{height:47; line-height:47px; background:url(../images/liimg03.jpg) 3px center no-repeat;} 
.iconul li.libg04{height:52; line-height:52px; background:url(../images/liimg04.jpg) 3px center no-repeat;} 

.contentbox{width:100%;min-height:450px; text-align:center;}
.news_contentbox{width:100%;min-height:400px; padding-top:30px; }
.news_left{width:201px; float:left; min-height:300px;margin-left:6px; position:relative;}
.news_left.news_leftdp{width:201px; float:left; min-height:300px;margin-left:6px; position:relative;}
.fix{position:fixed;top:0px;}
.news_left ul span{width:100%; display:block;font-size:18px;color:#006b63;margin-bottom:8px;border-bottom:1px solid #009da0; height:25px; line-height:25px;}
.news_left ul span.otherspan{border-bottom:none;}
.news_left ul li{width:100%;height:30px; line-height:30px;border-bottom:1px solid #009da0;}
.news_left ul li.otherli{ border-bottom:none; padding-left:5px;}
.news_left ul li a{ font-size:13px;color:#40b6b0; display:block;}
.news_left ul li a:hover{opacity:0.5;filter:progid:DXImageTransform.Microsoft.Alpha(Opacity=50);}

.backToTop{ position:fixed;width:60px;height:60px; bottom:0; right:0; background:url(../images/btn_backtop(09-30-13-10-24).gif);}
#bakd:hover{ opacity:0.5;}

#gototop { display:none; position:fixed; right:5px; bottom:5px; color:green; font-weight:bold; text-decoration:none; border:1px solid green; background:Lightgreen; padding:10px; }
#gototop { text-decoration:underline; }












.news_right{width:730px;min-height:400px; float:left;color:#000;margin-left:35px;}

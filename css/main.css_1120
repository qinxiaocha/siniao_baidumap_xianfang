/*footer css*/
.footer{width:100%;height:286px; padding-top:20px;font-size:12px; border-top:1px solid #ccc; font-family:"Microsoft Yahei"}
.footerlist{width:855px; margin:0 auto; margin-bottom:15px;}
.footerlist ul{ float:left; width:130px; margin-right:10px; overflow:hidden;}
.footerlist ul.otherul{ margin-right:0;width:125px; margin-left:20px;}
.footerlist ul img{ margin-bottom:13px; margin-left-:10px;}
.footerlist ul li{ background:url(../images/footerlist.jpg) left 5px no-repeat; padding-left:10px; line-height:22px;}
.footerlist ul li a{color:#333; display: inline-block;}
.footerlist ul li a:hover{filter:alpha(opacity=50); -moz-opacity:0.5;  opacity: 0.5;}
.linka{width:100%; text-align:center;}
.linka img{ margin-right:20px;}
